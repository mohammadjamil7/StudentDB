<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="266.0" prefWidth="419.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="university.LoginPageController">
    <Text layoutX="130" layoutY="50" style="-fx-font-size: 24;" text="Login" />
    
    <Text layoutX="50" layoutY="100" text="Username:" />
    <TextField fx:id="usernameField" layoutX="150" layoutY="85" prefWidth="200" />

    <Text layoutX="50" layoutY="150" text="Password:" />
    <PasswordField fx:id="passwordField" layoutX="150" layoutY="135" prefWidth="200" />

    <Button layoutX="150" layoutY="200" onAction="#handleLogin" prefHeight="25.0" prefWidth="200.0" text="Login" />
</AnchorPane>
